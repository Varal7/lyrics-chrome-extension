{
    "manifest_version": 2,

    "name": "Lyrics.ovh",
    "description": "This extension shows the lyrics for the current page",
    "version": "1.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://api.lyrics.ovh/"
    ]
}
