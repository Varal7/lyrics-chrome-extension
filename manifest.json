{
    "manifest_version": 2,

    "name": "Lyrics.ovh",
    "description": "This extension makes use of lyrics.ovh's API to display the lyrics of a YouTube's video",
    "version": "1.2",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://api.lyrics.ovh/",
        "https://api.alltomp3/"
    ]
}
